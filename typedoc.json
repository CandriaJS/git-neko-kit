  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "./docs",
  "includeVersion": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "plugin": ["typedoc-plugin-missing-exports", "typedoc-plugin-markdown"],
  "internalModule": "Map"
}